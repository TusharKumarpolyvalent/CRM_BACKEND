generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Campaign {
  id           String       @id      // manually provided, not autoincrement
  name         String
  client_name  String?
  status       String?
  description  String?
  start_date   DateTime? @db.Date
  end_date     DateTime? @db.Date
  meta_date    DateTime? @db.Date

  created_date   DateTime? @db.Date
  updated_date DateTime @updatedAt @db.Timestamp(0)
}

